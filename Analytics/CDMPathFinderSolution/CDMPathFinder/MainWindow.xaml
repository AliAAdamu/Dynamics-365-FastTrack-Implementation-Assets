<Window x:Class="CDMPathFinder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CDMPathFinder"
        mc:Ignorable="d"
        Title="CDMPathFinder - Dynamics 365 Finance and Operations applications" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tbxConnectionString" Margin="175,36,0,0" TextWrapping="Wrap" Text="" HorizontalAlignment="Left" Width="588" VerticalAlignment="Top" Height="22"/>
        <Label Content="Connection String" HorizontalAlignment="Left" Margin="10,32,0,0" VerticalAlignment="Top" Width="145"/>
        <TextBox x:Name="tbxContainerName" Margin="175,71,0,0" TextWrapping="Wrap" Text="" HorizontalAlignment="Left" Width="588" Height="22" VerticalAlignment="Top"/>
        <Label Content="Container Name" HorizontalAlignment="Left" Margin="10,67,0,0" VerticalAlignment="Top" Width="145"/>
        <TextBox x:Name="tbxTablesMainPath" Margin="175,107,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" HorizontalAlignment="Left" Width="588" Height="22"/>
        <Label Content="Tables Manifest Path" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top" Width="145"/>
        <Button x:Name="btnProcess" Content="Process" HorizontalAlignment="Left" Margin="10,137,0,0" Width="219" Click="Button_Click" Height="46" VerticalAlignment="Top"/>
        <TextBox x:Name="tbxReturn" Margin="10,188,10,10.5" TextWrapping="Wrap" Text="" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
